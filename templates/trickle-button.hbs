<div class="trickle {{_id}} {{_trickle2._button._component}}{{#if _trickle2._button._isLocking}} is-locked{{/if}}{{#if _trickle2._button._isFullWidth}} is-full-width{{/if}}{{#if _trickle2._button._className}} {{_trickle2._button._className}}{{/if}}">

  {{{a11y_aria_label _globals._extensions._trickle2.incompleteContent}}}

  <div class="trickle__inner js-trickle-btn-container {{_trickle2._button._component}}__inner {{#if _trickle2._button._isLocking}} is-locked{{/if}}{{#unless _trickle2._button._isVisible}} u-display-none{{/unless}}">

    <button class="trickle__btn js-trickle-btn {{#if _trickle2._button._isDisabled}} is-disabled{{/if}}{{#if _trickle2._button._hasIcon}} btn-icon{{/if}}{{#any _trickle2._button.finalText _trickle2._button.text _trickle2._button.startText}} btn-text{{/any}}"{{#if _trickle2._button._isDisabled}} disabled="disabled"{{else}} role="button"{{/if}}>

      {{#if _trickle2._button._hasIcon}}
      <div class="trickle__btn-icon">
        <div class="icon"></div>
      </div>
      {{/if}}

      {{#any _trickle2._button.finalText _trickle2._button.text _trickle2._button.startText}}
      <div class="trickle__btn-text">
        <div class="trickle__btn-text-inner">
          {{#if _trickle2._isFinal}}
            {{#if _trickle2._button.finalText}}
              {{{_trickle2._button.finalText}}}
            {{else}}
              {{#if _trickle2._button._isDisabled}}
		        			{{{_trickle2._button.disabledText}}}
		        		{{else}}
		        			{{{_trickle2._button.text}}}
		        		{{/if}}
            {{/if}}
          {{else}}
            {{#if _trickle2._isStart}}
              {{#if _trickle2._button.startText}}
                {{{_trickle2._button.startText}}}
              {{/if}}
            {{else}}
              {{#if _trickle2._button._isDisabled}}
		        			{{{_trickle2._button.disabledText}}}
		        		{{else}}
		        			{{{_trickle2._button.text}}}
		        		{{/if}}
            {{/if}}
          {{/if}}
        </div>
      </div>
      {{/any}}

    </button>

  </div>

</div>
